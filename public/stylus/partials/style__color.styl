//@Paleta de Colores 
$paper = #FFF

//@Color Primario
$ColorPrimario = #303F9F
$DefaultPrimaryColor = #3F51B5
$lightPimarioColor = #C5CAE9


//@Complementario
$Complementario = #03A9F4

//@borde de Paper
$border=#BDBDBD

//@Paleta de Textos
$TextosDefaul= #FFF
$TextoPrimario =#212121
$TextoSecundario =#757575


//@Global textual link color.
$linkcolor=  $ColorPrimario 
//@ Link hover color set via `saturation()` function.
$link-hover-color = saturation($linkcolor, 15%) 
//@ Link hover decoration.
$link-hover-decoration=underline 

//@BOX-SHADOW
$box-shadow = 0 3px 1px -2px rgba(0,0,0,.2), 0 2px 2px 0 rgba(0,0,0,.14), 0 1px 5px 0 rgba(0,0,0,.12)